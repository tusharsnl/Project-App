<body>
  <% include partials/nav.ejs %>
  <h1>Owner-Tenant Portal</h1>
  <h2>Welcome to Rest Communicator online facility</h2>
  <p>
    <div id="todo-table" align="center">
<span>Welcome Visitor<br></br></br></br></br></span>
<ol type="1">
  <% for(var i=0;i<resdata.length; i++){%>
    <li>Type: <%=resdata[i].type %></li></br></br>
    <li>Area: <%=resdata[i].area %></li></br></br>
    <li>Address: <%=resdata[i].address %></li></br></br>
    <li>Owner Name: <%=resdata[i].ownName %></li></br></br>
    <li>Status: <%=resdata[i].status %></li></br></br>
    <li>Contact: <%=resdata[i].contact %></li></br></br>
    <li>Rent: <%=resdata[i].rent %></li></br></br>
    <li>Security: <%=resdata[i].security %></li></br></br>
    <li>Features: <%=resdata[i].features %></li></br></br></br></br></br></br></br></br>
  <% } %>
</ol>

</div>
<h2> To view Properties click below </h2>
<form action=".\property-resedential" method="post" >
  <input type="radio" name="type" value="Resedential" checked> Resedential<br>
  <input type="radio" name="type" value="Commercial"> Commercial<br>
  <!-- <input type="radio" name="gender" value="other"> Other -->
  <button class="w3-btn w3-blue" id="SelectType">Selecty Type of Property</button>
